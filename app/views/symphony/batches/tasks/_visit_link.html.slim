- if action.completed?
  = render "symphony/batches/tasks/completed"
- else
  = link_to 'Start', action.task.link_url, target: :_blank, role: 'button', class: "btn btn-success btn-sm mb-2 #{"disabled" if !action.task.first? and (!action.task.check_previous.get_workflow_action(workflow.company, workflow.id)&.completed? or current_user.roles.exclude? action.task.role)}"