- if action.completed
  = render "symphony/batches/tasks/completed"
- else
  = link_to 'Start', new_symphony_invoice_path(workflow_name: workflow.template.slug, workflow_id: workflow.id, workflow_action_id: action.id, invoice_type: 'payable'), role: 'button', target: "_blank", class: 'btn btn-success btn-sm mb-2'